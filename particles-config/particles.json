{
    "fpsLimit": 60,
    "interactivity": {
        "events": {
            "onClick": {
                "mode": "push"
            },
            "onHover": {
                "enable": true,
                "mode": "none",
                "parallax": {
                    "enable": true,
                    "force": 15,
                    "smooth": 10
                }
            }
        },
        "modes": {
            "bubble": {
                "distance": 400,
                "duration": 2,
                "opacity": 0.8,
                "size": 40
            },
            "grab": {
                "distance": 400
            }
        }
    },
    "particles": {
        "color": {
            "value": "rgb(24, 31, 44)"
        },
        "links": {
            "color": {
                "value": "#ffffff"
            },
            "distance": 150,
            "opacity": 0.4
        },
        "move": {
            "attract": {
                "rotate": {
                    "x": 600,
                    "y": 1200
                }
            },
            "outModes": {
                "bottom": "out",
                "left": "out",
                "right": "out",
                "top": "out"
            }
        },
        "number": {
            "density": {
                "enable": true
            }
        },
        "opacity": {
            "random": {
                "enable": true
            },
            "value": 0.5,
            "animation": {
                "minimumValue": 0.1,
                "speed": 3
            }
        },
        "size": {
            "random": {
                "enable": true
            },
            "value": 10,
            "animation": {
                "enable": true,
                "minimumValue": 0.1,
                "speed": 20
            }
        },
        "stroke": {
            "color": {
                "value": "#000000",
                "animation": {
                    "enable": false,
                    "speed": 1,
                    "sync": true
                }
            }
        }
    }
}